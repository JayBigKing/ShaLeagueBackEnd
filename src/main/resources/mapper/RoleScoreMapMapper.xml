<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.shaleaguebackend.mapper.RoleScoreMapMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.shaleaguebackend.model.domain.RoleScoreMap">
        <result column="lordScore" property="lordScore" />
        <result column="courtierScore" property="courtierScore" />
        <result column="rebelScore" property="rebelScore" />
        <result column="traitorScore" property="traitorScore" />
    </resultMap>
    <select id = "getRoleScoreMap" resultMap="BaseResultMap">
        SELECT *
        FROM RoleScoreMap
    </select>
</mapper>
